[
  {
    "context": ["/google"],
    "target": "https://www.google.com",
    "secure": false,
    "autoRewrite": true,
    "pathRewrite": {
      "^/google": "/"
    },
    "rewriteHeaders": {
      "Host": "example.com"
    },
    "cookieDomainRewrite": { "*": "" }
  },
  {
    "context": ["/v1/app1"],
    "target": "http://localhost:1234",
    "secure": false,
    "pathRewrite": {
      "^/v1/app1": "/app1"
    }
  },
  {
    "context": ["/app2"],
    "target": "http://localhost:4001",
    "secure": false,
    "changeOrigin": true,
    "enforceAutoRewrite": true
  }
]
